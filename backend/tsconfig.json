{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": ["./node_modules/@types", "./node_modules/@prisma"]
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}